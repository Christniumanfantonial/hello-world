#include <stdio.h>

void main()
{
    int a[10];
    int i, n=5, sum=0;

       printf("size of array :");
       scanf("%d",&n);
       
       for(i=0; i<n; i++)
        {
	      printf("element %d: ",i+1);
	      scanf("%d",&a[i]);
	    }

    for(i=0; i<n; i++)
    {
        sum += a[i];
    }

    printf("Sum of all elements : %d\n", sum);
   
     return 0;
}
